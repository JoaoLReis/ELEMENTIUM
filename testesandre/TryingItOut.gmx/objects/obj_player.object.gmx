    sprite_index = sp_player_SW;
}
if(key_down != 0 &amp;&amp; key_right != 0)
{
    direction = 315;
    sprite_index = sp_player_SE;
}

if canmove
{
    x += xMove * moveSpeed;
    y += yMove * moveSpeed;
}

if(global.valueHealth == 0)
{
