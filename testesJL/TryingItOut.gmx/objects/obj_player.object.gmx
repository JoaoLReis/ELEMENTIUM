        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Keyboard Movement event Handler
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_up = -keyboard_check(vk_up);
key_down = keyboard_check(vk_down);

//step event
step_count = (step_count + 1) mod 14;

//Movement
if(canmove)
{
    if(key_up != 0 || keyboard_check(ord("W")))
    {
        SpriteDirection = 90;
        if place_free(x, y - 1)
            motion_add(90, acceleration)
        sprite_index = sp_player_N;
    }
    if(key_down != 0 || keyboard_check(ord("S")))
    {
        SpriteDirection = 270;
        if place_free(x, y + 1)
            motion_add(270, acceleration)
        sprite_index = sp_player_S;
    }
    if(key_left != 0 || keyboard_check(ord("A")))
    {
        SpriteDirection = 180;
        if place_free(x - 1,y)
            motion_add(180, acceleration)
        sprite_index = sp_player_W;
    }
    if(key_right != 0 || keyboard_check(ord("D")))
    {
        SpriteDirection = 0;
        if place_free(x + 1, y)
            motion_add(0, acceleration)
        sprite_index = sp_player_E;
    }
    if(key_up != 0 &amp;&amp; key_right != 0 || (keyboard_check(ord("W")) &amp;&amp; keyboard_check(ord("D"))))
    {
        SpriteDirection = 45;
        sprite_index = sp_player_NE;
    }
    if(key_up != 0 &amp;&amp; key_left != 0 || (keyboard_check(ord("W")) &amp;&amp; keyboard_check(ord("A"))))
    {
        SpriteDirection = 135;
        sprite_index = sp_player_NW;
    }
    if(key_down != 0 &amp;&amp; key_left != 0 || (keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("A"))))
    {
        SpriteDirection = 225;
        sprite_index = sp_player_SW;
    }
    if(key_down != 0 &amp;&amp; key_right != 0 || (keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("D"))))
    {
        SpriteDirection = 315;
        sprite_index = sp_player_SE;
    }
    if speed &gt; max_speed
    {
        speed = max_speed;
    }
}
else speed = 0;

if(speed == 0){
    if(direction &gt;= 0 &amp;&amp; direction &lt; 45 || direction &gt;= 315 &amp;&amp; direction &lt; 361)
        sprite_index = sp_player_E_IDLE;
    if(direction &gt;= 45 &amp;&amp; direction &lt; 135)
        sprite_index = sp_player_N_IDLE;
    if(direction &gt;= 135 &amp;&amp; direction &lt; 225)
        sprite_index = sp_player_W_IDLE;
    if(direction &gt;= 225 &amp;&amp; direction &lt; 315)
        sprite_index = sp_player_S_IDLE;
}
</string>
          </argument>
        </arguments>
      </action>
